. /etc/profile


export ORACLE_SID=prod
ORACLE_USER=backup_user
ORACLE_PWD=`get_pwd $ORACLE_USER`

mv  /work/orabackup/prod/dumpstore/* /orabackup/prod/dumpstore/

tsmclient_list1=`sqlplus -s $ORACLE_USER/$ORACLE_PWD << EOF 
set hea off
set feedback off
set pages 100
select ','||lower(username) from dba_users where username like 'TSM%' or username like 'FT15%';
EOF`

tsmclient_list=`echo $tsmclient_list1 | tr -d " "`

/u01/app/oracle/product/9.2.0/bin/exp USERID=$ORACLE_USER/$ORACLE_PWD \
OWNER=dmishra,ftcommon$tsmclient_list \
FILE=/work/orabackup/prod/dump/tsm_prod.dmp \
LOG=/export/home/oracle/log/tsm_prod.dmp.log

echo
echo
echo "List of users exported:" >> /export/home/oracle/log/tsm_prod.dmp.log
echo dmishra,ftcommon$tsmclient_list >> /export/home/oracle/log/tsm_prod.dmp.log

# rm -rf /orabackup/prod/hot/tsm_prod.dmp.Z

mv /work/orabackup/prod/dump/tsm_prod.dmp /work/orabackup/prod/dump/tsm_prod.dmp.$(date +%m%d)
mv /export/home/oracle/log/tsm_prod.dmp.log /export/home/oracle/log/tsm_prod.dmp.log.$(date +%m%d)

compress /work/orabackup/prod/dump/tsm_prod.dmp.$(date +%m%d)
# cp /work/orabackup/prod/dump/tsm_prod.dmp.$(date +%m%d).Z /orabackup/prod/hot/tsm_prod.dmp.Z

mv /work/orabackup/prod/dump/tsm_prod.dmp.$(date +%m%d).Z /work/orabackup/prod/dumpstore/


